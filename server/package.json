   "bugs": {
      "url": "https://github.com/ejson03/EHR/issues"
   },
   "dependencies": {
      "@types/passport": "^1.0.6",
      "@types/passport-local": "^1.0.33",
      "bigchaindb-driver": "^4.2.2",
      "connect-flash": "^0.1.1",
      "connect-mongo": "^4.4.1",
      "cors": "^2.8.5",
      "dotenv": "^9.0.0",
      "easy-bigchain": "^1.0.1",
      "ejs": "^3.1.6",
      "express": "^4.17.1",
      "express-session": "^1.17.1",
      "ipfs-api": "^26.1.2",
      "moment": "^2.29.1",
      "mongodb": "^3.6.6",
      "multer": "^1.4.2",
      "node-fetch": "^2.6.1",
