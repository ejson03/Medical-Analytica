      "@types/express": "^4.17.11",
      "@types/express-session": "^1.17.3",
      "@types/mongodb": "^3.6.12",
      "@types/mongoose": "^5.10.5",
      "@types/multer": "^1.4.5",
      "@types/node": "^15.0.2",
      "@types/node-fetch": "^2.5.10",
      "eslint": "^7.26.0",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "typescript": "^4.2.4"
   },
   "prettier": {
      "trailingComma": "none",
